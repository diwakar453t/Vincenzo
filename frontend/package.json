{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css,json}\"",
    "preview": "vite preview",
    "test": "npx vitest run",
    "test:unit": "npx vitest run src/tests/unit src/tests/components",
    "test:integration": "npx vitest run src/tests/integration",
    "test:watch": "npx vitest",
    "test:coverage": "npx vitest run --coverage",
    "test:ui": "npx vitest --ui",
    "e2e": "npx playwright test --project=chromium",
    "e2e:all": "npx playwright test",
    "e2e:headed": "npx playwright test --headed --project=chromium",
    "e2e:visual": "npx playwright test --project=visual-regression",
    "e2e:update-snapshots": "npx playwright test --project=visual-regression --update-snapshots",
    "e2e:install": "npx playwright install --with-deps",
    "e2e:report": "npx playwright show-report",
    "ci:test": "npx vitest run --coverage && echo 'Vitest passed'",
    "analyze": "vite build --mode analyze",
    "build:prod": "tsc -b && vite build --mode production"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@hookform/resolvers": "^5.2.2",
    "@mui/icons-material": "^7.3.8",
    "@mui/material": "^7.3.8",
    "@reduxjs/toolkit": "^2.11.2",
    "axios": "^1.13.6",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.19",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.71.1",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.13.0",
    "recharts": "^3.7.0",
    "yup": "^1.7.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.58.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "jsdom": "^28.1.0",
    "msw": "^2.12.10",
    "playwright": "^1.58.2",
    "prettier": "^3.8.1",
    "rollup-plugin-visualizer": "^7.0.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.48.0",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  }
}
