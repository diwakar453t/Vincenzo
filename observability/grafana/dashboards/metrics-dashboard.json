{
    "annotations": {
        "list": []
    },
    "editable": true,
    "fiscalYearStartMonth": 0,
    "graphTooltip": 1,
    "id": null,
    "links": [],
    "panels": [
        {
            "title": "Request Rate (req/s)",
            "type": "timeseries",
            "gridPos": {
                "h": 6,
                "w": 8,
                "x": 0,
                "y": 0
            },
            "datasource": "Prometheus",
            "targets": [
                {
                    "expr": "sum(rate(http_requests_total[5m]))",
                    "legendFormat": "Total",
                    "refId": "A"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "reqps",
                    "custom": {
                        "fillOpacity": 15
                    }
                }
            }
        },
        {
            "title": "P95 Latency",
            "type": "timeseries",
            "gridPos": {
                "h": 6,
                "w": 8,
                "x": 8,
                "y": 0
            },
            "datasource": "Prometheus",
            "targets": [
                {
                    "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[5m])) by (le))",
                    "legendFormat": "P95",
                    "refId": "A"
                },
                {
                    "expr": "histogram_quantile(0.50, sum(rate(http_request_duration_seconds_bucket[5m])) by (le))",
                    "legendFormat": "P50",
                    "refId": "B"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "s",
                    "custom": {
                        "fillOpacity": 10
                    }
                }
            }
        },
        {
            "title": "Error Rate (%)",
            "type": "gauge",
            "gridPos": {
                "h": 6,
                "w": 4,
                "x": 16,
                "y": 0
            },
            "datasource": "Prometheus",
            "targets": [
                {
                    "expr": "sum(rate(http_requests_total{status_code=~\"5..\"}[5m])) / sum(rate(http_requests_total[5m])) * 100",
                    "refId": "A"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "percent",
                    "max": 100,
                    "thresholds": {
                        "steps": [
                            {
                                "color": "green",
                                "value": null
                            },
                            {
                                "color": "yellow",
                                "value": 1
                            },
                            {
                                "color": "red",
                                "value": 5
                            }
                        ]
                    }
                }
            }
        },
        {
            "title": "In-Flight Requests",
            "type": "stat",
            "gridPos": {
                "h": 6,
                "w": 4,
                "x": 20,
                "y": 0
            },
            "datasource": "Prometheus",
            "targets": [
                {
                    "expr": "sum(http_requests_in_flight)",
                    "refId": "A"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "thresholds": {
                        "steps": [
                            {
                                "color": "green",
                                "value": null
                            },
                            {
                                "color": "yellow",
                                "value": 100
                            },
                            {
                                "color": "red",
                                "value": 200
                            }
                        ]
                    }
                }
            }
        },
        {
            "title": "Active Tenants (Colleges)",
            "type": "stat",
            "gridPos": {
                "h": 5,
                "w": 4,
                "x": 0,
                "y": 6
            },
            "datasource": "Prometheus",
            "targets": [
                {
                    "expr": "count(count by (tenant_id) (rate(tenant_requests_total[5m]) > 0))",
                    "refId": "A"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "color": {
                        "mode": "fixed",
                        "fixedColor": "blue"
                    }
                }
            }
        },
        {
            "title": "Auth Success Rate",
            "type": "gauge",
            "gridPos": {
                "h": 5,
                "w": 4,
                "x": 4,
                "y": 6
            },
            "datasource": "Prometheus",
            "targets": [
                {
                    "expr": "sum(rate(auth_attempts_total{result=\"success\"}[5m])) / sum(rate(auth_attempts_total[5m])) * 100",
                    "refId": "A"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "percent",
                    "max": 100,
                    "thresholds": {
                        "steps": [
                            {
                                "color": "red",
                                "value": null
                            },
                            {
                                "color": "yellow",
                                "value": 90
                            },
                            {
                                "color": "green",
                                "value": 99
                            }
                        ]
                    }
                }
            }
        },
        {
            "title": "DB Connection Pool",
            "type": "gauge",
            "gridPos": {
                "h": 5,
                "w": 4,
                "x": 8,
                "y": 6
            },
            "datasource": "Prometheus",
            "targets": [
                {
                    "expr": "db_connections_active / db_connections_pool_size * 100",
                    "refId": "A"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "percent",
                    "max": 100,
                    "thresholds": {
                        "steps": [
                            {
                                "color": "green",
                                "value": null
                            },
                            {
                                "color": "yellow",
                                "value": 70
                            },
                            {
                                "color": "red",
                                "value": 90
                            }
                        ]
                    }
                }
            }
        },
        {
            "title": "Requests by Tenant (Top 10)",
            "type": "barchart",
            "gridPos": {
                "h": 5,
                "w": 6,
                "x": 12,
                "y": 6
            },
            "datasource": "Prometheus",
            "targets": [
                {
                    "expr": "topk(10, sum(rate(tenant_requests_total[5m])) by (tenant_id))",
                    "legendFormat": "{{ tenant_id }}",
                    "refId": "A"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "reqps"
                }
            }
        },
        {
            "title": "Cache Hit Rate",
            "type": "stat",
            "gridPos": {
                "h": 5,
                "w": 6,
                "x": 18,
                "y": 6
            },
            "datasource": "Prometheus",
            "targets": [
                {
                    "expr": "sum(rate(cache_hits_total[5m])) / (sum(rate(cache_hits_total[5m])) + sum(rate(cache_misses_total[5m]))) * 100",
                    "refId": "A"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "percent",
                    "thresholds": {
                        "steps": [
                            {
                                "color": "red",
                                "value": null
                            },
                            {
                                "color": "yellow",
                                "value": 80
                            },
                            {
                                "color": "green",
                                "value": 95
                            }
                        ]
                    }
                }
            }
        },
        {
            "title": "HTTP Status Code Distribution",
            "type": "piechart",
            "gridPos": {
                "h": 6,
                "w": 6,
                "x": 0,
                "y": 11
            },
            "datasource": "Prometheus",
            "targets": [
                {
                    "expr": "sum(rate(http_requests_total[5m])) by (status_code)",
                    "legendFormat": "{{ status_code }}",
                    "refId": "A"
                }
            ]
        },
        {
            "title": "Request Latency by Endpoint (Top 10 Slowest)",
            "type": "table",
            "gridPos": {
                "h": 6,
                "w": 10,
                "x": 6,
                "y": 11
            },
            "datasource": "Prometheus",
            "targets": [
                {
                    "expr": "topk(10, histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[5m])) by (le, path_template)))",
                    "format": "table",
                    "instant": true,
                    "refId": "A"
                }
            ]
        },
        {
            "title": "DB Query Duration (P95)",
            "type": "timeseries",
            "gridPos": {
                "h": 6,
                "w": 8,
                "x": 16,
                "y": 11
            },
            "datasource": "Prometheus",
            "targets": [
                {
                    "expr": "histogram_quantile(0.95, sum(rate(db_query_duration_seconds_bucket[5m])) by (le, operation))",
                    "legendFormat": "{{ operation }}",
                    "refId": "A"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "s"
                }
            }
        },
        {
            "title": "CPU Usage",
            "type": "timeseries",
            "gridPos": {
                "h": 6,
                "w": 8,
                "x": 0,
                "y": 17
            },
            "datasource": "Prometheus",
            "targets": [
                {
                    "expr": "100 - (avg(rate(node_cpu_seconds_total{mode=\"idle\"}[5m])) * 100)",
                    "legendFormat": "CPU%",
                    "refId": "A"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "percent",
                    "max": 100
                }
            }
        },
        {
            "title": "Memory Usage",
            "type": "timeseries",
            "gridPos": {
                "h": 6,
                "w": 8,
                "x": 8,
                "y": 17
            },
            "datasource": "Prometheus",
            "targets": [
                {
                    "expr": "(1 - node_memory_MemAvailable_bytes / node_memory_MemTotal_bytes) * 100",
                    "legendFormat": "Memory%",
                    "refId": "A"
                },
                {
                    "expr": "redis_memory_used_bytes / 1024 / 1024",
                    "legendFormat": "Redis MB",
                    "refId": "B"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "percent",
                    "max": 100
                }
            }
        },
        {
            "title": "Disk Usage",
            "type": "gauge",
            "gridPos": {
                "h": 6,
                "w": 8,
                "x": 16,
                "y": 17
            },
            "datasource": "Prometheus",
            "targets": [
                {
                    "expr": "(1 - node_filesystem_avail_bytes{mountpoint=\"/\"} / node_filesystem_size_bytes{mountpoint=\"/\"}) * 100",
                    "refId": "A"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "percent",
                    "max": 100,
                    "thresholds": {
                        "steps": [
                            {
                                "color": "green",
                                "value": null
                            },
                            {
                                "color": "yellow",
                                "value": 70
                            },
                            {
                                "color": "red",
                                "value": 90
                            }
                        ]
                    }
                }
            }
        }
    ],
    "schemaVersion": 39,
    "tags": [
        "preskool",
        "metrics",
        "monitoring"
    ],
    "templating": {
        "list": [
            {
                "name": "tenant_id",
                "type": "query",
                "datasource": "Prometheus",
                "query": "label_values(tenant_requests_total, tenant_id)",
                "refresh": 2,
                "includeAll": true,
                "multi": true,
                "current": {
                    "selected": true,
                    "text": "All",
                    "value": "$__all"
                }
            }
        ]
    },
    "time": {
        "from": "now-1h",
        "to": "now"
    },
    "timepicker": {},
    "timezone": "browser",
    "title": "PreSkool ERP â€” Metrics & KPIs",
    "uid": "preskool-metrics",
    "version": 1
}